<%= simple_form_for(@trabajo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :cliente %>
    <%= f.input :descripcion %>
    <%= f.association :tipo_trabajo %>
    <%= f.input :fecha %>
    <%= f.input :valor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
