<%- model_class = Cliente -%>
<header class="header bg-info">
  <div class="row">
    <div class="col-sm-4">
      <h3 class="m-t m-b-none"><%=t '.title', :default => [:'helpers.titles.edit', 'Edit %{model}'], :model => Cliente %></h3>
      <p class="block text-muted">Ingrese los datos</p>
    </div>
  </div>
</header>
<div class="vbox">
    <div class="scrollable wrapper">
        <section class="panel">
            <header class="panel-heading">Ingrese los Datos </header>
            <div class="panel-body">
                <%= render :partial => 'form' %>
            </div>
        </section>
    </div>
</div>
