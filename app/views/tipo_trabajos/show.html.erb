<%- model_class = TipoTrabajo -%>
<div class="page-header">
  <h1><%=t '.title', :default => TipoTrabajo %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:nombre) %></dt>
      <dd><pre class="prettyprint"><%= @tipo_trabajo.nombre %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              tipo_trabajos_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tipo_trabajo_path(@tipo_trabajo), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tipo_trabajo_path(@tipo_trabajo),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

