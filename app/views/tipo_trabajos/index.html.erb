<h1>Listing tipo_trabajos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_trabajos.each do |tipo_trabajo| %>
      <tr>
        <td><%= tipo_trabajo.nombre %></td>
        <td><%= link_to 'Show', tipo_trabajo %></td>
        <td><%= link_to 'Edit', edit_tipo_trabajo_path(tipo_trabajo) %></td>
        <td><%= link_to 'Destroy', tipo_trabajo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo trabajo', new_tipo_trabajo_path %>
